{
  "name": "dralexgreaney.com",
  "version": "1.0.0",
  "description": "Dr. Greaney's Research Group Site",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "scss": "node-sass --output-style compact -o public/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r public/css/*",
    "watch-pug": "pug -O src/data.js src/index.pug --out public --watch --pretty",
    "uglify": "uglifyjs src/vendor/jquery.js src/vendor/tether.js src/vendor/*.js src/app.js -m -o public/build.js",
    "build:css": "npm run scss && npm run autoprefixer",
    "build:js": "npm run lint &amp;&amp; npm run uglify",
    "build-all": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autumnfjeld/dralexgreaney.git"
  },
  "keywords": [
    "Alex Greaney",
    "Computational Materials Science"
  ],
  "author": "Autumn Fjeld",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/autumnfjeld/dralexgreaney/issues"
  },
  "homepage": "https://github.com/autumnfjeld/dralexgreaney#readme",
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "cssmin": "^0.4.3",
    "node-sass": "^4.5.2",
    "postcss-cli": "^4.0.0",
    "pug": "^2.0.0-rc.1",
    "pug-cli": "^1.0.0-alpha6",
    "uglify-js": "^3.0.4"
  }
}
